<?xml version="1.0"?>
<!DOCTYPE import-control PUBLIC
        "-//Puppy Crawl//DTD Import Control 1.3//EN"
        "http://checkstyle.sourceforge.net/dtds/import_control_1_3.dtd">

<import-control pkg="com.helloowen.automation" strategyOnMismatch="allowed">
    <disallow pkg="com.codeborne.selenide"/>
    <disallow pkg="org.openqa.selenium"/>
    <subpackage name="pageobjects" strategyOnMismatch="allowed">
        <allow pkg="com.codeborne.selenide"/>
        <allow pkg="org.openqa.selenium"/>
    </subpackage>
</import-control>